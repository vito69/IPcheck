{% extends 'base.html.twig' %}

{% block title %}Check other IP address{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('css/style.css') }}">
    <link rel="stylesheet" href="{{ asset('css/menu.css') }}">
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('js/jquery.min.js') }}"></script>
    <script src="{{ asset('js/main.js') }}"></script>
{% endblock %}
{% block theme 'checkIP' %}
{% block body %}

    {{ include ('menu.html.twig') }}
    <div class="container">
        <h1>Wprowad&#378; adres IP: </h1>
        <div class='well'>
            {{ include ('form4ip.html.twig') }}
        </div>
    {% if isp is defined %}
        <h1>ISP: </h1>
        {{ isp | raw }}
    {% endif %}
    </div>
{% endblock %}
